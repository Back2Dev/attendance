{
  "name": "back2bikes-attendance",
  "private": false,
  "version": "2.0.0",
  "scripts": {
    "visualize": "meteor --production --extra-packages bundle-visualizer --settings dev.mikkel.settings.json",
    "newman": "newman run tests/agentbox/postman/webhooks.postman_collection.json -e tests/agentbox/postman/se-webhooks.postman_environment.json",
    "debug": "DEBUG=b2b:* MONGO_URL=mongodb://localhost/b2b meteor --port 4080 --exclude-archs web.cordova,web.browser.legacy --settings dev.settings.json --inspect",
    "nodebug": "DEBUG=b2b:* MONGO_URL=mongodb://localhost/b2bprod meteor --port 4080 --exclude-archs web.cordova,web.browser.legacy --settings dev.settings.json ",
    "debug-local": "DEBUG=b2b:* meteor --inspect --port 4080 --settings dev.settings.json",
    "build": "meteor build ../builds/",
    "ci-app": "DEBUG=b2b:* METEOR_ALLOW_SUPERUSER=1 meteor --port 4080 --settings dev.circleci.settings.json",
    "ci": "DEBUG=b2b:* METEOR_ALLOW_SUPERUSER=1 meteor --port 4080 --settings dev.circleci.settings.json",
    "ci-test": "./node_modules/cypress/bin/cypress run --config integrationFolder=tests/cypress/integration/working/",
    "storybook": "start-storybook -s public -p 3076",
    "sb": "start-storybook -s ./public -p 3076",
    "storybook:build": "build-storybook -c .storybook -s ./public",
    "test": "npm run test:unit && npm run test:storyshots && npm run test:integration",
    "test:integration": "meteor test --port 4085 --once --driver-package meteortesting:mocha --inspect --settings dev.settings.json",
    "test:integration:ci": "meteor test --port 4085 --once --driver-package meteortesting:mocha --inspect --settings dev.circleci.settings.json",
    "test:integration:ci:watch": "TEST_WATCH=1 meteor test --port 4085 --once --driver-package meteortesting:mocha --inspect --settings dev.circleci.settings.json",
    "test:integration:debug": "meteor test --inspect --port 4085 --once --driver-package meteortesting:mocha --settings dev.settings.json",
    "test:integration:browser": "meteor test --port 4085 --once --driver-package practicalmeteor:mocha --settings dev.settings.json",
    "test:integration:watch": "TEST_WATCH=1 meteor test --port 4085 --inspect --driver-package meteortesting:mocha --settings dev.settings.json",
    "test:integration:watch:debug": "DEBUG=b2b:* TEST_WATCH=1 meteor test --port 4085 --inspect --driver-package meteortesting:mocha --settings dev.settings.json",
    "test:integration:watch:browser": "TEST_WATCH=1 meteor test --port 4085 --driver-package practicalmeteor:mocha --settings dev.settings.json",
    "test:storyshots": "jest tests/snapshots/**",
    "test:snapshots": "jest tests/snapshots/**",
    "test:storyshots:watch": "jest tests/snapshots/** --watchAll",
    "test:storyshots:update": "jest tests/snapshots/** --updateSnapshot",
    "test:unit": "jest tests/unit/**",
    "test:unit:watch": "jest tests/unit/** --watchAll",
    "test:unit:watch:debug": "DEBUG=b2b:* mocha --opts tests/mocha.opts --watch",
    "test:e2e:standalone": "node tests/cypress/run-on-circle.js 'meteor npm run ci-app' 'meteor npm run ci-test'",
    "test:e2e:standalone:survey": "node tests/cypress/run-on-circle.js 'meteor npm run ci-app' 'meteor npm run ci-test-survey'",
    "test:packages": "meteor --settings dev.mikkel.settings.json test-packages ./packages/slingshot --driver-package meteortesting:mocha",
    "cypress:open": "./node_modules/cypress/bin/cypress open --config integrationFolder=tests/cypress/integration/",
    "cypress:run": "./node_modules/cypress/bin/cypress run --config integrationFolder=tests/cypress/integration/working/",
    "cypress:nw": "./node_modules/cypress/bin/cypress run --config integrationFolder=tests/cypress/integration/not-working/",
    "cypress:record": "./node_modules/cypress/bin/cypress run --config integrationFolder=tests/cypress/integration --record --key 7f96e2e6-b3ad-4fdc-8b70-af7cfc31f7da",
    "mockserver": "mockserver -p 8080 -m ./tests/api/mocks/"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^3.2.10",
    "@sentry/node": "^5.29.0",
    "autosuggest-highlight": "^3.1.1",
    "aws-sdk": "^2.807.0",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "change-case": "^4.1.2",
    "d3": "^6.3.1",
