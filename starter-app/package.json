    "react-codejar": "^1.0.2",
    "react-data-grid": "^7.0.0-canary.33",
    "react-dom": "^16.13.1",
    "react-dropzone": "^11.2.4",
    "react-google-button": "^0.7.1",
    "react-helmet": "^6.1.0",
    "react-livechat": "^1.1.1",
    "react-markdown": "^5.0.3",
    "react-material-ui-form-validator": "^2.1.1",
    "react-pdf": "^5.1.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-tabulator": "^0.14.2",
    "react-toastify": "^6.2.0",
    "simpl-schema": "^1.10.2",
    "styled-components": "^5.2.1",
    "uniforms-bridge-json-schema": "^3.0.0-rc.8",
    "uniforms-bridge-simple-schema-2": "^3.0.0-rc.8",
    "uniforms-material": "^3.0.0-rc.8",
    "winston": "=3.1.0",
    "winston-loggly-bulk": "git+https://github.com/loggly/winston-loggly-bulk.git#3.x",
    "winston-mongodb": "^5.0.5"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    }
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-arrays": "^2.2.0",
    "cypress": "^6.8.0",
    "cypress-file-upload": "^5.0.2",
    "cypress-fill-command": "^1.0.2",
    "eslint": "^7.15.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "nock": "^13.0.11",
    "sinon": "^9.2.1"
  }
