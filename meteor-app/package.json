    "debug-local-pa": "cross-env DEBUG=b2b:* meteor --inspect --port 3030 --settings dev.pa.settings.json",
    "debug.email": "MAIL_URL=smtps://admin@back2bikes.com.au:Back2bikes.24@secure364.sgcpanel.com:465 DEBUG=b2b:* MONGO_URL=mongodb://localhost:27017/b2b meteor --inspect --port 3030 --settings dev.settings.json",
    "debug.pa": "DEBUG=b2b:* MONGO_URL=mongodb://localhost:27017/paprod meteor --exclude-archs web.cordova,web.browser.legacy --inspect --port 3030 --settings dev.pa.settings.json",
