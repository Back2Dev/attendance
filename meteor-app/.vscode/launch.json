{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "server",
      "restart": true,
      "port": 9229,
      "timeout": 60000
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Launch: Chrome",
      "sourceMaps": true,
      "url": "http://localhost:3030/kiosk",
      "webRoot": "${workspaceFolder}",
      "outputCapture": "std"
    },
    {
      "type": "chrome",
      "request": "attach",
      "sourceMaps": true,
      "name": "Attach Chrome /kiosk",
      "url": "http://localhost:3030/kiosk",
      "webRoot": "${workspaceFolder}",
      "run chrome like this:": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome --remote-debugging-port=9222",
      "navigate to:": "http://localhost:3030/kiosk",
      "port": 9222
    },
    {
      "type": "chrome",
      "request": "attach",
      "sourceMaps": true,
      "name": "Attach to /service",
      "url": "http://localhost:3030/service",
      "webRoot": "${workspaceFolder}",
      "run chrome like this:": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome --remote-debugging-port=9222",
      "navigate to:": "http://localhost:3030/ (home page)",
      "port": 9222
    },
    {
      "type": "chrome",
      "request": "attach",
      "sourceMaps": true,
      "name": "Attach to useradmin",
      "url": "http://localhost:3030/useradmin",
      "webRoot": "${workspaceFolder}",
      "run chrome like this:": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome --remote-debugging-port=9222",
      "navigate to:": "http://localhost:3030/ (home page)",
      "port": 9222
    },
    {
      "type": "chrome",
      "request": "attach",
      "sourceMaps": true,
      "name": "Attach to admin/matching",
      "url": "http://localhost:3030/admin/matching",
      "webRoot": "${workspaceFolder}",
      "run chrome like this:": "/Applications/Google Chrome.app/Contents/MacOS/Google Chrome --remote-debugging-port=9222",
      "navigate to:": "http://localhost:3030/ (home page)",
      "port": 9222
    }
  ],
  "compounds": [
    {
      "name": "Meteor: All",
      "configurations": [
        "server",
        "Attach Chrome"
      ]
    }
  ]
}