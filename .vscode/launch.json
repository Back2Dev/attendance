{
    "version": "0.2.0",
    "configurations": [
      {
          "type": "node",
          "request": "attach",
          "name": "server",
          "restart": true,
          "port": 9229,
      },
      {
          "type": "chrome",
          "request": "attach",
          "name": "Attach Chrome",
          "url": "http://localhost:3010",
          "webRoot": "${workspaceFolder}",
          "run chrome like this:": "/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome",
          "navigate to:": "http://localhost:3010/ (login page)",
          "port": 9232
      },
      {
          "type": "chrome",
          "request": "launch",
          "name": "Launch Chrome",
          "url": "http://localhost:3010",
          "webRoot": "${workspaceFolder}"
      },
      {
          "type": "node",
          "request": "launch",
          "name": "Meteor: Server Tests",
          "runtimeExecutable": "npm",
          "restart": true,
          "runtimeArgs": [
            "run",
            "test:integration:debug"
          ],
          "port": 9230,
          "timeout": 240000
      },
  ],
  "compounds": [
      {
          "name": "Meteor: All",
          "configurations": ["server", "Attach Chrome"]
      }
  ]
  }