{
  "name": "back2bikes-attendance",
  "private": false,
  "version": "2.0.0",
  "scripts": {
    "visualize": "meteor --production --extra-packages bundle-visualizer --settings dev.mikkel.settings.json",
    "newman": "newman run tests/agentbox/postman/webhooks.postman_collection.json -e tests/agentbox/postman/se-webhooks.postman_environment.json",
    "debug": "DEBUG=app:* MONGO_URL=mongodb://localhost/b2b meteor --port 4090 --exclude-archs web.cordova,web.browser.legacy --settings dev.settings.json --inspect",
    "android": "DEBUG=app:* MONGO_URL=mongodb://localhost/b2b meteor run android-device --port 4090 --settings dev.settings.json --inspect --mobile-server=https://demo.back2bikes.com.au ",
    "nodebug": "DEBUG=app:* MONGO_URL=mongodb://localhost/b2b meteor --port 4090 --exclude-archs web.cordova,web.browser.legacy --settings dev.settings.json ",
    "debug-ios": "meteor run ios-device --mobile-server 192.168.1.4:3000 --settings dev.settings.json --exclude-archs web.browser",
    "debug-local": "cross-env DEBUG=app:* meteor --inspect --port 4090 --settings dev.settings.json",
    "build": "meteor build ../builds/",
    "ci-app": "DEBUG=app:* METEOR_ALLOW_SUPERUSER=1 meteor --port 4090 --settings dev.circleci.settings.json",
    "ci": "DEBUG=app:* METEOR_ALLOW_SUPERUSER=1 meteor --port 4090 --settings dev.circleci.settings.json",
    "ci-test": "./node_modules/cypress/bin/cypress run --config integrationFolder=tests/cypress/integration/working/",
    "storybook": "start-storybook -s public -p 3076",
    "sb": "start-storybook -s ./public -p 3076",
    "storybook:build": "build-storybook -c .storybook -s ./public",
    "storybook:publish": "chromatic",
    "test": "npm run test:unit && npm run test:storyshots && npm run test:integration",
    "test:integration": "meteor test --port 4095 --once --driver-package meteortesting:mocha --inspect --settings dev.settings.json",
    "test:integration:ci": "meteor test --port 4095 --once --driver-package meteortesting:mocha --inspect --settings dev.circleci.settings.json",
    "test:integration:ci:watch": "TEST_WATCH=1 meteor test --port 4095 --once --driver-package meteortesting:mocha --inspect --settings dev.circleci.settings.json",
    "test:integration:debug": "meteor test --inspect --port 4095 --once --driver-package meteortesting:mocha --settings dev.settings.json",
    "test:integration:browser": "meteor test --port 4095 --once --driver-package practicalmeteor:mocha --settings dev.settings.json",
    "test:integration:watch": "TEST_WATCH=1 meteor test --port 4095 --inspect --driver-package meteortesting:mocha --settings dev.settings.json",
    "test:integration:watch:debug": "DEBUG=app:* TEST_WATCH=1 meteor test --port 4095 --inspect --driver-package meteortesting:mocha --settings dev.settings.json",
    "test:integration:watch:browser": "TEST_WATCH=1 meteor test --port 4095 --driver-package practicalmeteor:mocha --settings dev.settings.json",
    "test:storyshots": "jest tests/snapshots/**",
    "test:snapshots": "jest tests/snapshots/**",
    "test:storyshots:watch": "jest tests/snapshots/** --watchAll",
    "test:storyshots:update": "jest tests/snapshots/** --updateSnapshot",
    "test:unit": "jest tests/unit/**",
    "test:unit:watch": "jest tests/unit/** --watchAll",
    "test:unit:watch:debug": "DEBUG=app:* mocha --opts tests/mocha.opts --watch",
    "test:unit:ui": "jest --testMatch \"$(pwd)/imports/ui/**/*.test.js\" --watchAll",
    "test:unit:ui:debug": "node --inspect-brk jest --runInBand --testMatch \"$(pwd)/imports/ui/**/*.test.js\" --watchAll",
    "test:e2e:standalone": "node tests/cypress/run-on-circle.js 'meteor npm run ci-app' 'meteor npm run ci-test'",
    "test:e2e:standalone:survey": "node tests/cypress/run-on-circle.js 'meteor npm run ci-app' 'meteor npm run ci-test-survey'",
    "test:packages": "meteor --settings dev.mikkel.settings.json test-packages ./packages/slingshot --driver-package meteortesting:mocha",
    "mockserver": "mockserver -p 8080 -m ./tests/api/mocks/",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "patch-package"
  },
